{

	"manifest_version": 2,
	"name": "Furaffinity Tags Blocker",
	"version": "0.1",

	"description": "",

	"icons": {
		"128": "icon-128.png"
	},

	"content_scripts": [
		{
			"matches": [
				"*://www.furaffinity.net/*"
			],
			"js": [
				"api.js",
				"submission_checker.js",
				"content.js"
			]
		}
	],

	"permissions": [
		"*://www.furaffinity.net/*",
		"https://cors.kona.workers.dev/*"
	]
}


